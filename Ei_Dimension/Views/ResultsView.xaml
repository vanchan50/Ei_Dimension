<UserControl x:Class="Ei_Dimension.Views.ResultsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:ViewModels="clr-namespace:Ei_Dimension.ViewModels"
             xmlns:lang="clr-namespace:Ei_Dimension.Language;assembly=Language"
             mc:Ignorable="d"
             xmlns:img="clr-namespace:Ei_Dimension.Styles"
             xmlns:System="clr-namespace:System;assembly=mscorlib"
             DataContext="{dxmvvm:ViewModelSource Type=ViewModels:ResultsViewModel}"
             d:DesignHeight="944" d:DesignWidth="1920">
  <Grid>
    <Border HorizontalAlignment="Left" VerticalAlignment="Top"
            Margin="750,0,0,0" Width="470" Height="900">
      <Grid>

        <Button Width="470" Height="50"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  FontSize="36"
                  Content="{Binding PlexButtonString}"
                  Style="{StaticResource ButtonStyle}"
                  Command="{Binding PlexButtonClickCommand}"/>
        <StackPanel Margin="0,48,0,4"
                    Visibility="{Binding MultiPlexVisible}">
          <Border Height="45"
                HorizontalAlignment="Left">
            <Grid>
              <TextBlock Text="{lang:Loc Name}"
                         Width="178"
                         FontSize="30"
                         TextAlignment="Center"
                         HorizontalAlignment="Left"/>
              <TextBlock Text="{lang:Loc Count}"
                         FontSize="30"
                         Width="119"
                         Margin="178,0,0,0"
                         TextAlignment="Center"
                         HorizontalAlignment="Left"/>
              <TextBlock Text="{lang:Loc Mean}"
                         FontSize="30"
                         Width="155"
                         Margin="317,0,0,0" 
                         TextAlignment="Left"
                         HorizontalAlignment="Left"/>
            </Grid>
          </Border>
          <ScrollViewer HorizontalScrollBarVisibility="Disabled"
                    VerticalScrollBarVisibility="Visible" Height="803">
            <ListBox Name="Table" Background="{StaticResource RibbonBackgroundActive}">
              <ListBox.ItemsPanel>
                <ItemsPanelTemplate>
                  <WrapPanel Width="440"/>
                </ItemsPanelTemplate>
              </ListBox.ItemsPanel>
            </ListBox>
          </ScrollViewer>
        </StackPanel>
        <!--Calibration Statistics-->
        <Border HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  Margin="0,50,0,0"
                  Visibility="{Binding SinglePlexVisible}">
          <StackPanel Orientation="Horizontal"
                  HorizontalAlignment="Stretch"
                  VerticalAlignment="Stretch">
            <StackPanel HorizontalAlignment="Right"
                    Margin="0,56,4,0">
              <TextBlock Text="{lang:Loc DataAn_Green_SSC}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"/>
              <TextBlock Text="{lang:Loc DataAn_Green_Maj}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,14,0,0"/>
              <TextBlock Text="{lang:Loc DataAn_Green_Min}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,14,0,0"/>
              <TextBlock Text="{lang:Loc DataAn_Red_SSC}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,18,0,0"/>
              <TextBlock Text="{lang:Loc CL1}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,14,0,0"/>
              <TextBlock Text="{lang:Loc CL2}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,14,0,0"/>
              <TextBlock Text="{lang:Loc CL3}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,16,0,0"/>
              <TextBlock Text="{lang:Loc DataAn_Violet_SSC}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,16,0,0"/>
              <TextBlock Text="{lang:Loc CL0}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,14,0,0"/>
              <TextBlock Text="{lang:Loc DataAn_FSC}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 Margin="0,14,0,0"/>
            </StackPanel>
            <StackPanel>
              <TextBlock Text="{lang:Loc DataAn_MFI}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Center"
                 Margin="0,0,0,4"/>
              <Border Background="{StaticResource RibbonBackgroundActive}">
                <StackPanel VerticalAlignment="Stretch"
                    Width="150">
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,8,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[0]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[1]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[2]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[3]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[4]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[5]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[6]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[7]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[8]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,8"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding MfiItems[9]}"/>
                </StackPanel>
              </Border>
            </StackPanel>
            <StackPanel>
              <TextBlock Text="{lang:Loc DataAn_CV}"
                 FontFamily="{StaticResource AppTextFont}"
                 FontSize="36"
                 FontWeight="UltraBold"
                 Foreground="{StaticResource AppTextColor}"
                 HorizontalAlignment="Center"
                 Margin="0,0,0,4"/>
              <Border Background="{StaticResource RibbonBackgroundActive}">
                <StackPanel VerticalAlignment="Stretch"
                    Width="150">
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,8,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[0]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[1]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[2]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[3]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[4]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[5]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[6]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[7]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,20"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[8]}"/>
                  <TextBox HorizontalAlignment="Stretch"
                      Margin="8,0,8,8"
                      IsReadOnly="True"
                      Style="{StaticResource InputFieldStyle}"
                      Text="{Binding CvItems[9]}"/>
                </StackPanel>
              </Border>
            </StackPanel>
          </StackPanel>
        </Border>
      </Grid>
    </Border>

    <!--Scatter Chart-->
    <Grid MouseEnter="Grid_MouseEnter"
          MouseLeave="Grid_MouseLeave">
      <dxc:ChartControl Width="740" Height="472"
                      HorizontalAlignment="Left"
                      VerticalAlignment="Top"
                      Focusable="False" Name="ScatterPlot"
                      BorderThickness="0">
        <dxc:ChartControl.Legends>
          <dxc:Legend HorizontalPosition="Left"
                    MarkerMode="CheckBox"
                    Orientation="Horizontal"
                    Margin="-30,-38,0,0"
                    FontSize="24"
                    Background="Transparent"
                    BorderBrush="Transparent"
                    VerticalPosition="Top"
                    Focusable="False"
                    Width="525" Visible="True"/>
        </dxc:ChartControl.Legends>
        <dxc:XYDiagram2D Focusable="False"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Top"
                         Margin="60,0,0,0">
          <dxc:XYDiagram2D.AxisY>
            <dxc:AxisY2D Interlaced="False"
                       GridLinesVisible="False"
                       TickmarksMinorVisible="False"
                       FontFamily="{StaticResource AppTextFont}"
                       TickmarksVisible="False">
              <dxc:AxisY2D.Label>
                <dxc:AxisLabel FontSize="24"/>
              </dxc:AxisY2D.Label>
              <dxc:AxisY2D.Title>
                <dxc:AxisTitle Content="{lang:Loc Count}"
                             FontSize="30"
                             FontWeight="UltraBold"
                             Visible="True"/>
              </dxc:AxisY2D.Title>
            </dxc:AxisY2D>
          </dxc:XYDiagram2D.AxisY>
          <dxc:XYDiagram2D.AxisX>
            <dxc:AxisX2D TickmarksMinorVisible="False"
                       TickmarksVisible="False"
                       Logarithmic="True"
                       FontFamily="{StaticResource AppTextFont}"
                       GridLinesMinorVisible="True"
                       GridLinesVisible="True"
                       MinorCount="9">
              <dxc:AxisX2D.Label>
                <dxc:AxisLabel FontSize="24"/>
              </dxc:AxisX2D.Label>
              <dxc:AxisX2D.Title>
                <dxc:AxisTitle Content="{lang:Loc Value}"
                             FontSize="30"
                             FontWeight="UltraBold"
                             Visible="True"/>
              </dxc:AxisX2D.Title>
              <dxc:AxisX2D.Range>
                <dxc:AxisRange SideMarginsEnabled="False"/>
              </dxc:AxisX2D.Range>
            </dxc:AxisX2D>
          </dxc:XYDiagram2D.AxisX>
          <dxc:LineSeries2D DataSource="{Binding DisplayedRedSsc}"
                          ValueDataMember="Value"
                          ArgumentDataMember="Argument"
                          DisplayName="Red"
                          Brush="Red"
                          CheckedInLegend="{Binding ScatterSelectorState[0]}"/>
          <dxc:LineSeries2D DataSource="{Binding DisplayedGreenSsc}"
                          ValueDataMember="Value"
                          ArgumentDataMember="Argument"
                          DisplayName="Green"
                          Brush="Green"
                          CheckedInLegend="{Binding ScatterSelectorState[1]}"/>
          <dxc:LineSeries2D DataSource="{Binding DisplayedVioletSsc}"
                          ValueDataMember="Value"
                          ArgumentDataMember="Argument"
                          DisplayName="Violet"
                          Brush="DarkViolet"
                          CheckedInLegend="{Binding ScatterSelectorState[2]}"/>
          <dxc:LineSeries2D DataSource="{Binding DisplayedForwardSsc}"
                          ValueDataMember="Value"
                          ArgumentDataMember="Argument"
                          DisplayName="Forward"
                          Brush="Chocolate"
                          CheckedInLegend="{Binding ScatterSelectorState[3]}"/>
          <dxc:LineSeries2D DataSource="{Binding DisplayedReporter}"
                          ValueDataMember="Value"
                          ArgumentDataMember="Argument"
                          DisplayName="Reporter"
                          Brush="Black"
                          CheckedInLegend="{Binding ScatterSelectorState[4]}"/>
        </dxc:XYDiagram2D>
      </dxc:ChartControl>
      <Button Width="50" Height="50"
            HorizontalAlignment="Left"
            VerticalAlignment="Top" Visibility="Hidden"
            Margin="665,40,0,0" Name="printSC"
            Command="{Binding ScatterprintCommand}">
        <Button.Content>
          <Image Stretch="Fill" Source="{dx:DXImage 'SvgImages/XAF/Action_Export_ToImage.svg'}"/>
        </Button.Content>
      </Button>
    </Grid>
    
    <!--Heatmap Chart-->
    <Grid MouseEnter="XYPlot_MouseEnter"
          MouseLeave="XYPlot_MouseLeave">
      <dxc:ChartControl Width="660" Height="412"
                      HorizontalAlignment="Left"
                      VerticalAlignment="Top"
                      BorderThickness="0" Name="XYPlot"
                      Focusable="False"
                      Margin="80,444,0,0">
        <dxc:XYDiagram2D Focusable="False" Name="ChC"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top">
          <dxc:XYDiagram2D.AxisY>
            <dxc:AxisY2D Logarithmic="True"
                       GridLinesMinorVisible="True"
                       MinorCount="9"
                       Interlaced="False"
                       TickmarksMinorVisible="False"
                       FontFamily="{StaticResource AppTextFont}"
                       TickmarksVisible="False">
              <dxc:AxisY2D.Label>
                <dxc:AxisLabel FontSize="24"/>
              </dxc:AxisY2D.Label>
              <dxc:AxisY2D.Title >
                <dxc:AxisTitle Content="{Binding CLAxis[1]}"
                             FontSize="30"
                             FontWeight="UltraBold"
                             Visible="False"/>
              </dxc:AxisY2D.Title>
              <dxc:AxisY2D.WholeRange>
                <dxc:Range dxc:AxisY2D.AlwaysShowZeroLevel="False" >
                  <dxc:Range.MinValue>
                    <System:Double>10</System:Double>
                  </dxc:Range.MinValue>
                </dxc:Range>
              </dxc:AxisY2D.WholeRange>
            </dxc:AxisY2D>
          </dxc:XYDiagram2D.AxisY>
          <dxc:XYDiagram2D.AxisX>
            <dxc:AxisX2D Logarithmic="True"
                       GridLinesMinorVisible="True"
                       MinorCount="9"
                       FontFamily="{StaticResource AppTextFont}"
                       GridLinesVisible="True"
                       TickmarksMinorVisible="False"
                       TickmarksVisible="False">
              <dxc:AxisX2D.Label>
                <dxc:AxisLabel FontSize="24"/>
              </dxc:AxisX2D.Label>
              <dxc:AxisX2D.Title>
                <dxc:AxisTitle Content="{Binding CLAxis[0]}"
                             FontSize="30"
                             FontWeight="UltraBold"
                             Visible="False"/>
              </dxc:AxisX2D.Title>
              <dxc:AxisX2D.WholeRange>
                <dxc:Range>
                  <dxc:Range.MinValue>
                    <System:Double>10</System:Double>
                  </dxc:Range.MinValue>
                </dxc:Range>
              </dxc:AxisX2D.WholeRange>
            </dxc:AxisX2D>
          </dxc:XYDiagram2D.AxisX>
          <dxc:PointSeries2D DataSource="{Binding WorldMap}"
                           ArgumentDataMember="X" CrosshairContentShowMode="Legend"
                           ValueDataMember="Y"
                           MarkerSize="5"
                           Brush="LightGray">
            <dxc:PointSeries2D.MarkerModel>
              <dxc:BorderlessSimpleMarker2DModel/>
            </dxc:PointSeries2D.MarkerModel>
          </dxc:PointSeries2D>
          <dxc:PointSeries2D Name="HeatMap" DisplayName="HeatMap" MarkerSize="3">
            <dxc:PointSeries2D.MarkerModel>
              <dxc:BorderlessSimpleMarker2DModel/>
            </dxc:PointSeries2D.MarkerModel>
          </dxc:PointSeries2D>
        </dxc:XYDiagram2D>
      </dxc:ChartControl>
      <Button Width="50" Height="50"
            HorizontalAlignment="Left"
            VerticalAlignment="Top" Visibility="Hidden"
            Margin="175,470,0,0" Name="printXY"
            Command="{Binding XYprintCommand}">
        <Button.Content>
          <Image Stretch="Fill" Source="{dx:DXImage 'SvgImages/XAF/Action_Export_ToImage.svg'}"/>
        </Button.Content>
      </Button>
    </Grid>
    <StackPanel HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Margin="15,550,0,0">
      <RadioButton Width="80" Height="50"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Content="CL0"
                   FontSize="35" Cursor="Hand"
                   IsChecked="{Binding CLButtonsChecked[0], Mode=OneWay}"
                   Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                   Command="{Binding CLButtonClickCommand}">
        <RadioButton.CommandParameter>
          <System:Int32>0</System:Int32>
        </RadioButton.CommandParameter>
      </RadioButton>
      <RadioButton Width="80" Height="50"
                   Margin="0,6,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Content="CL1"
                   FontSize="35" Cursor="Hand"
                   IsChecked="{Binding CLButtonsChecked[1], Mode=OneWay}"
                   Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                   Command="{Binding CLButtonClickCommand}">
        <RadioButton.CommandParameter>
          <System:Int32>1</System:Int32>
        </RadioButton.CommandParameter>
      </RadioButton>
      <RadioButton Width="80" Height="50"
                   Margin="0,6,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Content="CL2"
                   FontSize="35" Cursor="Hand"
                   IsChecked="{Binding CLButtonsChecked[2], Mode=OneWay}"
                   Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                   Command="{Binding CLButtonClickCommand}">
        <RadioButton.CommandParameter>
          <System:Int32>2</System:Int32>
        </RadioButton.CommandParameter>
      </RadioButton>
      <RadioButton Width="80" Height="50"
                   Margin="0,6,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Content="CL3"
                   FontSize="35" Cursor="Hand"
                   IsChecked="{Binding CLButtonsChecked[3], Mode=OneWay}"
                   Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                   Command="{Binding CLButtonClickCommand}">
        <RadioButton.CommandParameter>
          <System:Int32>3</System:Int32>
        </RadioButton.CommandParameter>
      </RadioButton>
    </StackPanel>

    <StackPanel Orientation="Horizontal"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Margin="250,850,0,0">
      <RadioButton Width="80" Height="50"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Content="CL0"
                   FontSize="35" Cursor="Hand"
                   IsChecked="{Binding CLButtonsChecked[4], Mode=OneWay}"
                   Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                   Command="{Binding CLButtonClickCommand}">
        <RadioButton.CommandParameter>
          <System:Int32>4</System:Int32>
        </RadioButton.CommandParameter>
      </RadioButton>
      <RadioButton Width="80" Height="50"
                   Margin="6,0,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Content="CL1"
                   FontSize="35" Cursor="Hand"
                   IsChecked="{Binding CLButtonsChecked[5], Mode=OneWay}"
                   Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                   Command="{Binding CLButtonClickCommand}">
        <RadioButton.CommandParameter>
          <System:Int32>5</System:Int32>
        </RadioButton.CommandParameter>
      </RadioButton>
      <RadioButton Width="80" Height="50"
                   Margin="6,0,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Content="CL2"
                   FontSize="35" Cursor="Hand"
                   IsChecked="{Binding CLButtonsChecked[6], Mode=OneWay}"
                   Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                   Command="{Binding CLButtonClickCommand}">
        <RadioButton.CommandParameter>
          <System:Int32>6</System:Int32>
        </RadioButton.CommandParameter>
      </RadioButton>
      <RadioButton Width="80" Height="50"
                   Margin="6,0,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Content="CL3"
                   FontSize="35" Cursor="Hand"
                   IsChecked="{Binding CLButtonsChecked[7], Mode=OneWay}"
                   Style="{StaticResource RadioButtonHorizontalThemeStyle}"
                   Command="{Binding CLButtonClickCommand}">
        <RadioButton.CommandParameter>
          <System:Int32>7</System:Int32>
        </RadioButton.CommandParameter>
      </RadioButton>
    </StackPanel>
    
    <!-- Company Logo Left -->
    <Image HorizontalAlignment="Left" VerticalAlignment="Top"
           Margin="10,898,0,0"
           Width="430" Height="40"
           Source="/Icons/dimension flow analyzer logo.png"
           RenderOptions.BitmapScalingMode="HighQuality"/>
    
    <!--Plate pictogram-->
    <Border Width="780" Height="524"
            Margin="1230,372,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top">
      <Grid>
        <StackPanel Orientation="Horizontal"
                    Margin="52,-10,0,0"
                    Visibility="{Binding LeftLabel384Visible}"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Height="43">
          <TextBlock Text="1"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"/>
          <TextBlock Text="2"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="34,0,0,0"/>
          <TextBlock Text="3"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="28,0,0,0"/>
          <TextBlock Text="4"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="27,0,0,0"/>
          <TextBlock Text="5"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="30,0,0,0"/>
          <TextBlock Text="6"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="28,0,0,0"/>
          <TextBlock Text="7"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="31,0,0,0"/>
          <TextBlock Text="8"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="28,0,0,0"/>
          <TextBlock Text="9"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="30,0,0,0"/>
          <TextBlock Text="10"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="20,0,0,0"/>
          <TextBlock Text="11"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Margin="16,0,0,0"/>
          <TextBlock Text="12"
                   FontFamily="{DynamicResource AppTextFont}"
                   FontSize="40"
                   FontWeight="ExtraBold"
                   Foreground="{DynamicResource AppTextColor}"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
